{
    "name": "@rcade/vite-plugins",
    "version": "0.1.0",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "build": "esbuild ./src/index.ts --bundle --outdir=./dist --platform=node && pnpm run build:types",
        "build:types": "tsc --emitDeclarationOnly --declaration --outDir dist"
    },
    "dependencies": {
        "vite-plugin-static-copy": "^2.0.0"
    },
    "peerDependencies": {
        "pyodide": "^0.27.0",
        "vite": "^5.0.0 || ^6.0.0 || ^7.0.0"
    },
    "devDependencies": {
        "@types/node": "^22.0.0",
        "typescript": "^5.0.0",
        "vite": "^7.2.4",
        "pyodide": "^0.27.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
