{
	"name": "web-content",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"preview": "pnpm --filter @rcade/api run build && wrangler dev",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check .",
		"deploy": "wrangler deploy"
	},
	"devDependencies": {
		"@sveltejs/adapter-cloudflare": "^7.2.4",
		"@sveltejs/kit": "^2.48.5",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@types/pako": "^2.0.3",
		"svelte": "^5.43.8",
		"svelte-check": "^4.3.4",
		"typescript": "^5.9.3",
		"vite": "^7.2.2",
		"wrangler": "^4.54.0"
	},
	"dependencies": {
		"@rcade/log": "workspace:*",
		"@rcade/api": "workspace:*",
		"@rcade/engine": "workspace:*",
		"modern-tar": "^0.7.2",
		"pako": "^2.1.0"
	}
}
