{
	"name": "@rcade/web",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"preview": "bun run --cwd ../api build && wrangler dev",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check .",
		"deploy": "wrangler deploy",
		"db:generate": "drizzle-kit generate",
		"db:migrate:local": "wrangler d1 migrations apply rcade --local",
		"db:migrate:remote": "wrangler d1 migrations apply rcade --remote",
		"db:studio": "drizzle-kit studio",
		"db:pull": "bun run scripts/pull-db.js",
		"analyze": "vite build --mode production && npx vite-bundle-visualizer"
	},
	"devDependencies": {
		"@libsql/client": "^0.15.15",
		"@sveltejs/adapter-cloudflare": "^7.2.4",
		"@sveltejs/kit": "^2.48.5",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.17",
		"@types/semver": "^7.7.1",
		"drizzle-kit": "^0.31.7",
		"drizzle-orm": "^0.44.7",
		"jose": "^6.1.2",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.43.8",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^4.1.17",
		"typescript": "^5.9.3",
		"vite": "^7.2.2",
		"wrangler": "^4.50.0",
		"zod": "^4.1.13"
	},
	"dependencies": {
		"@auth/sveltekit": "^1.11.1",
		"@aws-sdk/client-s3": "^3.940.0",
		"@aws-sdk/s3-request-presigner": "^3.940.0",
		"@cloudflare/workers-types": "^4.20251126.0",
		"@isomorphic-git/lightning-fs": "^4.6.2",
		"@rcade/api": "workspace:*",
		"@rcade/log": "workspace:*",
		"@rcade/engine": "workspace:*",
		"isomorphic-git": "^1.35.1",
		"memfs": "^4.51.0",
		"semver": "^7.7.3"
	}
}