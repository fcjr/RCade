{
  "name": "rcade-monorepo",
  "private": true,
  "workspaces": [
    "web",
    "cabinet",
    "cli",
    "create",
    "action-deploy",
    "api",
    "runtime",
    "sdk/frontend/typescript",
    "sdk/backend",
    "plugins/input-classic/clients/typescript",
    "plugins/input-classic",
    "plugins/input-spinners/clients/typescript",
    "plugins/input-spinners",
    "plugins/sleep/clients/typescript",
    "plugins/sleep",
    "plugins/menu/clients/typescript",
    "plugins/menu",
    "vite-plugins",
    "web-content",
    "engine",
    "menu",
    "log"
  ],
  "scripts": {
    "dev:web": "bun run --cwd web dev",
    "dev:client": "bun run --cwd client dev",
    "dev:menu": "bun run dev-menu.ts",
    "build:cli": "bun run --cwd cli build",
    "check": "bun run --cwd web check",
    "prepare": "husky"
  },
  "dependencies": {
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/bun": "^1.3.4",
    "husky": "^9.1.7"
  }
}